{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/docs",
      "destination": "https://docs-shipkit.vercel.app/"
    },
    {
      "source": "/docs/:match*",
      "destination": "https://docs-shipkit.vercel.app/:match*"
    },
    {
      "source": "/blog",
      "destination": "https://blog-shipkit.vercel.app/"
    },
    {
      "source": "/blog/:match*",
      "destination": "https://blog-shipkit.vercel.app/:match*"
    },
    {
      "source": "/ui",
      "destination": "https://ui-shipkit.vercel.app/"
    },
    {
      "source": "/ui/:match*",
      "destination": "https://ui-shipkit.vercel.app/:match*"
    },
    {
      "source": "/tools",
      "destination": "https://tools-shipkit.vercel.app/"
    },
    {
      "source": "/tools/:match*",
      "destination": "https://tools-shipkit.vercel.app/:match*"
    },
    {
      "source": "/",
      "destination": "https://main-shipkit.vercel.app/"
    },
    {
      "source": "/:match*",
      "destination": "https://main-shipkit.vercel.app/:match*"
    }
  ]
}